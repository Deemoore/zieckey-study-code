LINKER_FLAGS = /PDBTYPE:CON
WORD = 32
!IF "$(DDKBUILDENV)" == "checked"
CHECK=chk
!ELSE
CHECK=fre
!ENDIF

!ENDIF

TARGETNAME=RSA$(CHECK)$(WORD)
TARGETPATH=..\..\360GPUB\lib
TARGETTYPE=DRIVER_LIBRARY

