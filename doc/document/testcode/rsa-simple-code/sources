!IF "$(BUILD_DEFAULT_TARGETS)" == "-amd64"

C_DEFINES = /D_WDK
WORD = 64
CHECK = $(DDKBUILDENV)

!ELSEIF "$(BUILD_DEFAULT_TARGETS)" == "-386"

